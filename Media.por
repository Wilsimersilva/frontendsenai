programa {
  funcao inicio() {
    real num1, num2, num3, media

    escreva(" Digite o valor do numero um: ")
    leia (num1)
    escreva(" Digite o valor do numero dois: ")
    leia (num2)
    escreva(" Digite o valor do numero tres: ")
    leia (num3)

    media = (num1 + num2 +num3)/3

    escreva("A média é: ", media)
  }
}
